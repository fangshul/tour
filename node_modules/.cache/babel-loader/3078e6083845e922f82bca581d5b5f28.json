 (result.body.data) {\r\n\t\t\t\t\t\tthis.replayLen = result.body.data.length\r\n\t\t\t\t\t\tthis.dataList = result.body.data\r\n\t\t\t\t\t}\r\n\t\t\t\t\t\r\n\t\t\t\t}else {\r\n\t\t\t\t\tthis.replayLen = 0\r\n\t\t\t\t}\r\n\t\t\t})\r\n\t\t}\r\n\t}\r\n</script>\r\n<style type=\"text/css\">\r\n\t.replayBox{\r\n\t\twidth: 715px;\r\n\t\tmin-height: 449px;\r\n\t\tborder:1px solid #ccc;\r\n\t\tpadding: 20px;\r\n\t\tmargin-bottom: 20px;\r\n\t}\r\n\t.perreplay{\r\n\t\twidth: 756px;\r\n\t\tmargin: 0 auto;\r\n\t}\r\n\t.perreplay>p{\r\n\t\tmargin-bottom: 20px;\r\n\t}\r\n\t.replayItem_1>span,\r\n\t.replayItem_2>span{\r\n\t\tcolor: #7086c6;\r\n\t\tcursor: pointer;\r\n\t}\r\n\t.replayItem_1,\r\n\t.replayItem_2{\r\n\t\theight: 40px;\r\n\t\tline-height: 40px;\r\n\t\tdisplay: inline-block;\r\n\t\tborder-bottom:1px solid #000;\r\n\t}\r\n</style>"],"sourceRoot":"src/components"}]}